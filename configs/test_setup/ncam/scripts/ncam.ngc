#<_rough_cut>    =  1.000000

#<_finish_feed>  =  0.050000
#<_finish_cut>   =  0.254000

o<tool_change_0011> if [1 EQ 0]
	G96 D800 S250
o<tool_change_0011> else
	G97 S800
o<tool_change_0011> endif

o<tool_change_0012> if [#<_spindle_dir> GT 0]
	M#<_spindle_dir>
	G4 P#<_spindle_speed_up_delay>
o<tool_change_0012> endif

